{
  "extends": [
    "development"
  ],
  "hints": {
    "axe/forms": "off",
    "axe/name-role-value": "off",
    "axe/text-alternatives": "off",
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "-webkit-overflow-scrolling"
        ]
      }
    ]
  },
  "browserslist": [
    "defaults",
    "not ie 11",
    "not and_ff <= 127",
    "not firefox <= 127",
    "not ios_saf <= 17.5",
    "not safari <= 17.5"
  ]
}